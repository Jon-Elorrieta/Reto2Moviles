<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TableLayout
        android:id="@+id/tablaMediciones"
        android:layout_width="360dp"
        android:layout_height="691dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginHorizontal="20dp"
        android:layout_marginVertical="20dp">

        <TableRow>

            <TextView
                android:id="@+id/textViewHora"
                android:layout_width="wrap_content"
                android:layout_height="29dp"
                android:text="Hora" />

            <TextView
                android:id="@+id/textViewCOmgm31"
                android:layout_width="97dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.5"
                android:paddingTop="5dp"
                android:paddingBottom="5dp"
                android:text="COmgm3" />

            <TextView
                android:id="@+id/textViewNOgm3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="NOgm3" />

            <TextView
                android:id="@+id/textViewNO2ICA"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="NO2ICA" />

            <TextView
                android:id="@+id/textViewPM10ICA"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="PM10ICA" />

            <TextView
                android:id="@+id/textViewNO2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="NO2" />

            <TextView
                android:id="@+id/textViewICAEstacion"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ICAEstacion" />


        </TableRow>


    </TableLayout>


</RelativeLayout>